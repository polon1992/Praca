{\cs12\f2\fs24\cf1\i0 \b \ul0 Trapez_2:=proc(a,b,c,d,N_x,N_y) local h,k,i,j,p,q,
x,y,z,T,Tp; h:=(b-a)/N_x; k:=(d-c)/N_y; for i from 0 to N_x do x:= i -> a+i*h od
;  for j from 0 to N_y do y:= j -> c+j*k od; for i from 0 to N_x do for j from 0
 to N_y do z:= (i,j) -> f(x(i),y(j)) od od; T:= h*k/4 * sum(sum(z(p,q) + z(p+1,q
) + z(p,q+1) + z(p+1,q+1), p=0..N_x-1), q=0..N_y-1); Tp:=evalf(T); end;
}
